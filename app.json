{
  "expo": {
    "name": "locastium",
    "slug": "locastium",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/astem-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/astem-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mhrrmsmsk.locastium"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/astem-logo.png",
        "backgroundColor": "#ffffff"
      },
      "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "package": "com.mhrrmsmsk.locastium"
    },
    "web": {
      "favicon": "./assets/astem-logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "7e1302e2-5845-41ff-9952-a472e277479a"
      }
    }
  }
}